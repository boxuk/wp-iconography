<?xml version="1.0"?>
<ruleset name="BoxUK">
	<file>./src</file>
	<file>./tests</file>
	<arg name="colors" />
	<arg name="parallel" value="80" />
	<arg value="sp" />
	<arg name="cache" value=".phpcs-cache" />

	<exclude-pattern>vendor</exclude-pattern>

	<rule ref="WordPress-VIP-Go" />
	<rule ref="Generic.Files.EndFileNewline" />
</ruleset>
